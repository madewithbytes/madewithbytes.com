body{font-family:Geneva,lucida sans,lucida grande,lucida sans unicode,Verdana,sans-serif;height:100%;background:#f0f0d8;color:#666}a{text-decoration:none;color:#666;padding:3px 7px;background-color:#b4debe}a:link,a:visited{color:#666}a:hover,a:active{background-color:#b4debe}h1,h2,h3,h4,h5{font-family:vt323,Garamond,hoefler text,Palatino,palatino linotype,serif;font-weight:400;font-size:2.8em}h2,h3,h4,h5{font-size:3em}.wrapper{width:90%;margin:0 auto;display:grid;grid-template-columns:50% 50%}.row{grid-column:1/3}header{margin:2rem 0 0;height:3rem}.logo{float:left;margin:0;font-size:1.6em}nav{float:right}nav ul{margin:0}nav li{display:inline}